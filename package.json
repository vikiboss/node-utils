{
  "name": "@vmoe/node-utils",
  "version": "0.0.0",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "description": "common node utils in pure ESM format",
  "keywords": [
    "utils",
    "toolkit"
  ],
  "author": "Viki <hi@viki.moe> (https://github.com/vikiboss)",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": null,
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "dev": "npm run build -- --watch src",
    "build": "tsup src/index.ts --format esm --dts --clean"
  },
  "dependencies": {
    "@types/gradient-string": "^1.1.2",
    "@types/kind-of": "^6.0.0",
    "@types/node": "^18.15.11",
    "@types/node-cron": "^3.0.7",
    "@types/tinycolor2": "^1.4.3",
    "axios": "^1.3.5",
    "colorette": "^2.0.19",
    "dayjs": "^1.11.7",
    "gradient-string": "^2.0.2",
    "kind-of": "^6.0.3",
    "mkdirp": "^3.0.0",
    "mri": "^1.2.0",
    "nanoid": "^4.0.2",
    "node-cron": "^3.0.2",
    "rimraf": "^5.0.0",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
